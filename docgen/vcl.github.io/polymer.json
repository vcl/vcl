{
  "root": "",
  "entrypoint": "index.html",
  "shell": "web-components/doc-index.js",
  "moduleResolution": "node",
  "npm": true,
  "autoBasePath": false,
  "extraDependencies": [
    "node_modules/@webcomponents/webcomponentsjs/**"
  ],
  "builds": [
    {
      "name": "dist",
      "addServiceWorker": false,
      "preset": "es5-bundled",
      "js": {"minify": false, "compile": false},
      "css": {"minify": false},
      "html": {"minify": false},
      "addPushManifest": false,
      "bundle": {
        "inlineCss": true,
        "inlineScripts": false,
        "rewriteUrlsInTemplates": false,
        "sourcemaps": true
      }
    }
  ]
}

